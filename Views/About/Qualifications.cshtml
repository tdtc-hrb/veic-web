@{
    var title = "";
    var link = Context.Request.Path.Value;
    if (link.Contains("qualifications"))
        title = "企业资质";
    else
        title = "技术创新";
}

@section Breadcrumb {
    @await Component.InvokeAsync("Breadcrumb", title)
}

@model IEnumerable<veic_web.Models.Wraps.ExhibitDto>
<section>
    @foreach (var item in Model)
    {
        <div>
            <img src="@Url.Content("../img/" + @item.ImgName)" />
            <p> @item.Name</p>
        </div>
    }
</section>

